AA,IAAA;;AACA,WAAA,IAAA,CAAA,GAAA,CAAA,EAAA,CAAA,GAAA,KAAA,CAAA,MAAA,EAAA,CAAA,EAAA,EAAA;AACA,YAAA,KAAA,CAAA,CAAA,CAAA,CAAA,IAAA,IAAA,KAAA,CAAA,UAAA,MAAA,KAAA,CAAA,CAAA,CAAA,CAAA,IAAA,EAAA;AACA,UAAA,IAAA,CAAA,IAAA,CAAA;AACA,YAAA,IAAA,EAAA,KAAA,CAAA,CAAA,CAAA,CAAA,IADA;AAEA,YAAA,KAAA,EAAA,KAAA,CAAA,CAAA,CAAA,CAAA,KAFA;AAGA,YAAA,IAAA,EAAA,KAAA,CAAA,CAAA,CAAA,CAAA;AAHA,WAAA;AAKA,SANA,MAOA,IAAA,CAAA,KAAA,CAAA,CAAA,CAAA,CAAA,IAAA,EAAA;AACA,UAAA,IAAA,CAAA,IAAA,CAAA;AACA,YAAA,IAAA,EAAA,KAAA,CAAA,CAAA,CAAA,CAAA,IADA;AAEA,YAAA,KAAA,EAAA,KAAA,CAAA,CAAA,CAAA,CAAA,KAFA;AAGA,YAAA,IAAA,EAAA;AAHA,WAAA;AAKA;AACA;;AACA,MAAA,KAAA,CAAA,KAAA,GAAA,IAAA;AACA,KApBA;AAqBA,GAjCA;AAkCA,EAAA,OAAA,EAAA;AACA,IAAA,UADA,wBACA;AACA,UAAA,IAAA,GAAA,IAAA,IAAA,EAAA;AAEA,UAAA,IAAA,GAAA,IAAA,CAAA,WAAA,EAAA;AAEA,UAAA,KAAA,GAAA,IAAA,CAAA,QAAA,KAAA,CAAA;;AACA,UAAA,KAAA,GAAA,CAAA,EAAA;AACA,QAAA,KAAA,GAAA,MAAA,KAAA;AACA;;AAEA,UAAA,GAAA,GAAA,IAAA,CAAA,OAAA,EAAA;;AACA,UAAA,GAAA,GAAA,CAAA,EAAA;AACA,QAAA,GAAA,GAAA,MAAA,GAAA;AACA;;AAEA,aAAA,KAAA,GAAA,GAAA,GAAA,GAAA,GAAA,GAAA,GAAA,IAAA;AACA;AAjBA;AAlCA,CAAA","sourcesContent":["<template>\n    <div>\n        <v-card>\n            <v-card-title>\n                <v-layout justify-center><h2>Upcoming Games</h2></v-layout>\n            </v-card-title>\n            <v-data-table v-bind:headers=\"game_headers\" v-bind:items=\"games\" :sort-by=\"['date']\">\n            </v-data-table>\n        </v-card>\n    </div>\n</template>\n\n<script>\nexport default {\n    data: function() {\n        return {\n            game_headers: [\n                { text: \"Name\", value: \"name\"},\n                { text: \"Type of game\", value: \"style\"},\n                { text: \"Date\", value: \"date\"}\n            ],\n            games: []\n        }\n    },\n    mounted: function() {\n        this.$axios.get(\"games/upcoming\").then(response => {\n            var game = [];\n            const games = response.data;\n            for (var i = 0; i < games.length; i++) {\n                if (games[i].date && this.createDate() <= games[i].date) {\n                    game.push({\n                        name: games[i].name,\n                        style: games[i].style,\n                        date: games[i].date\n                    });\n                }\n                else if (!games[i].date){\n                    game.push({\n                        name: games[i].name,\n                        style: games[i].style,\n                        date: \"TBD\"\n                    })\n                }\n            }\n            this.games = game;\n        })\n    },\n    methods: {\n        createDate() {\n            const date = new Date();\n\n            var year = date.getFullYear();\n\n            var month = date.getMonth() + 1;\n            if (month < 9) {\n                month = \"0\" + (month);\n            }\n\n            var day = date.getDate();\n            if (day < 9) {\n                day = \"0\" + day;\n            }\n\n            return month + \"-\" + day + \"-\" + year;\n        }\n    }\n}\n</script>"],"sourceRoot":"src/components"}]}