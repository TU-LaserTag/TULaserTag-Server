AAYA,MAAA,KAAA,EAAA,EAZA;AAaA,MAAA,KAAA,EAAA;AAbA,KAAA;AAeA,GAlBA;AAmBA,EAAA,OAAA,EAAA;AACA,IAAA,MADA,oBACA;AAAA;;AACA,WAAA,MAAA,CAAA,GAAA,kBAAA,KAAA,QAAA,cAAA,KAAA,QAAA,GAAA,IAAA,CAAA,UAAA,QAAA,EAAA;AACA,YAAA,QAAA,CAAA,IAAA,CAAA,EAAA,EAAA;AACA,cAAA,KAAA,GAAA,QAAA,CAAA,IAAA,CAAA,MAAA,CAAA,KAAA,CAAA,KAAA;;AACA,cAAA,KAAA,EAAA;AACA,YAAA,KAAA,CAAA,QAAA,GAAA,IAAA;AACA,WAFA,MAGA;AACA,YAAA,KAAA,CAAA,MAAA,GAAA,IAAA;AACA,YAAA,KAAA,CAAA,OAAA,GAAA,qDAAA;AACA;AACA,SATA,MAUA,KAAA,CAAA,OAAA,GAAA,QAAA,CAAA,IAAA,CAAA,OAAA;AACA,OAZA;AAaA,KAfA;AAgBA,IAAA,SAhBA,uBAgBA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,cAAA;AACA,KAlBA;AAmBA,IAAA,SAnBA,uBAmBA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,cAAA;AACA,KArBA;AAsBA,IAAA,WAtBA,yBAsBA;AACA,WAAA,KAAA,GAAA,SAAA;AACA,KAxBA;AAyBA,IAAA,WAzBA,yBAyBA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,aAAA;AACA,KA3BA;AA4BA,IAAA,QA5BA,sBA4BA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,aAAA;AACA,KA9BA;AA+BA,IAAA,iBA/BA,+BA+BA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,sBAAA;AACA;AAjCA;AAnBA,CAAA","sourcesContent":["<template>\n    <v-container>\n        <v-layout justify-center>\n            <v-card v-if=!signedIn>\n                <v-toolbar>\n                    <v-toolbar-title>Sign in</v-toolbar-title>\n                </v-toolbar>\n                <v-card-text>\n                    <v-form ref=\"form\" v-model=\"valid\">\n                        <v-text-field label=\"Username\" name=\"username\" v-model=\"username\" :rules=\"usernameRules\" required></v-text-field>\n                        <v-text-field label=\"Password\" name=\"password\" type=\"password\" v-model=\"password\" :rules=\"passwordRules\" required></v-text-field>\n                    </v-form>\n                    <v-spacer />\n                    <span style=\"color:red; font-size:17px\">{{message}}</span>\n                </v-card-text>\n                <v-card-actions>\n                    <v-spacer />\n                    <v-btn v-on:click=\"submit\" :disabled=\"(!valid || banned)\">Submit</v-btn>\n                </v-card-actions>\n            </v-card>\n        </v-layout>\n        <v-toolbar flat v-if=signedIn>\n            <span class='pa-2' style='font-size:32px'> Hello, <span style='font-style:italic'>{{username}}!</span></span>    \n            <v-spacer />\n            <span class=mr-12><font style=font-size:28px>Options:</font></span>\n\n            <v-btn class=ma-2 color=\"#61578b\" raised v-on:click=\"viewTeams\">\n                <font color=\"white\">Teams</font>\n            </v-btn>\n\n            <v-btn class=ma-2 color=\"#61578b\" raised v-on:click=\"viewPlayers\">\n                <font color=\"white\">Players</font>\n            </v-btn>\n        \n            <v-btn class=ma-2 color=\"#61578b\" raised v-on:click=\"viewGames\">\n                <font color=white>Games</font>\n            </v-btn>\n\n            <v-btn class=ma-2 color=\"#61578b\" raised v-on:click=\"viewLeagues\">\n                <font color=\"white\" >Leagues</font>\n            </v-btn>\n\n            <v-btn class=ma-2 color=\"#61578b\" raised v-on:click=\"viewGuns\">\n                <font color=\"white\">guns</font>\n            </v-btn>\n\n            <v-btn class=ma-2 color=\"#61578b\" raised v-on:click=\"viewAnnouncements\">\n                <font color=white>Announcements</font>\n            </v-btn>\n        </v-toolbar>\n        <br><br>\n        <v-row>\n            <v-layout justify-center>\n                <div v-if=\"(table == 'players')\">\n                    <player-table></player-table>\n                </div>\n            </v-layout>\n        </v-row>\n\n    </v-container>\n</template>\n\n<script>\nimport PlayerTable from \"../components/PlayerTable\";\nexport default {\n    components: {PlayerTable},\n    data: function() {\n        return {\n            signedIn: true, // CHANGE TO FALSE WHEN DONE\n            banned: false,\n            username: \"The Admin\",\n            usernameRules: [\n                v => !!v || 'Username is required'\n            ],\n            password: \"\",\n            passwordRules: [\n                v => !!v || 'Password is required'\n            ],\n            message:\"\",\n            table:\"\",\n            valid: false\n        }\n    },\n    methods: {\n        submit() {\n            this.$axios.get(`player/${this.username}/${this.password}`).then(response => {\n                if (response.data.ok) {\n                    const admin = response.data.person.roles.admin;\n                    if (admin) {\n                        this.signedIn = true;\n                    }\n                    else {\n                        this.banned = true;\n                        this.message = \"Not an admin. Please contact an admin to become one\"\n                    }\n                }\n                else this.message = response.data.message;\n            })\n        },\n        viewTeams() {\n            console.log(\"Teams worked\");\n        },\n        viewGames() {\n            console.log(\"Games worked\");\n        },\n        viewPlayers() {\n            this.table = \"players\"\n        },\n        viewLeagues() {\n            console.log(\"Guns worked\");\n        },\n        viewGuns() {\n            console.log(\"Guns worked\");\n        },\n        viewAnnouncements() {\n            console.log(\"Announcements worked\");\n        }\n    }\n}\n</script>"],"sourceRoot":"src/pages"}]}